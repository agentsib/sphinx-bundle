{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon">
            {{ include('AgentSIBSphinxBundle:Profiler:icon.svg.twig') }}
            <span class="sf-toolbar-ajax-requests sf-toolbar-status sf-toolbar-status-green">{{ collector.queriesCount }}</span>
        </span>
    {% endset %}
    {% if collector.queriesCount and false %}
        {{ include('WebProfilerBundle:Profiler:toolbar_item.html.twig', { link: false }) }}
    {% endif %}
{% endblock %}